QPushButton
{
	font-family:"Microsoft Yahei";
	border:1px solid rgb(222, 222, 222);
	border-radius:3px;
	background-color:rgb(250, 250 ,250);
	color:rgb(104, 104, 104);
}

QPushButton:hover
{
	font-family:"Microsoft Yahei";
	border:1px solid rgb(175, 210, 235);
	background-color:rgb(247, 252 ,255);
	color:rgb(121, 175, 213);
}

QPushButton:pressed
{
	font-family:"Microsoft Yahei";
	border:1px solid rgb(95, 166, 215);
	background-color:rgb(214, 238 ,252);
	color:rgb(40, 125, 184);
}

QLabel
{
	font-family:"Microsoft Yahei";
	color:rgb(58, 58, 58);
}

QLabel#label_title
{
	font-family:"Microsoft Yahei";
